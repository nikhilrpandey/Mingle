<link rel="stylesheet" href="/styles/sign-in.css">
<h1>
    Sign up
</h1>
<div class="sign-up">
    <form action="/user/create" method="POST">
        <label for="user-id">Email</label>
        <input type="email" name="userId" id="" placeholder="email" required > <br>
        <label for="user-passwd">Password</label>
        <input type="password" name="userPassword" id="" placeholder="password" required><br>
        <label for="user-name">Username</label>
        <input type="text" name="username" id="" placeholder="username" required><br>
        <input type="button" value="Sign Up"> 
    </form>
</div>
